<Window x:Class="Global.UserControls.Pixel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Global.UserControls"
        mc:Ignorable="d"
        Title="Pixel" Height="110" Width="80">
    <Grid>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
            </Grid.RowDefinitions>
            <Grid  Grid.Row="1">
                <TextBlock Name="textBlockXY" Foreground="Black"></TextBlock>
            </Grid>
            <Grid  Grid.Row="2">
                <TextBlock Name="textBlockRGB" Foreground="Black"></TextBlock>
            </Grid>
        </Grid>
    </Grid>
</Window>
