<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    xmlns:local="clr-namespace:ConfigObjective">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/ConfigObjective;component/themes/Base.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <Style x:Key="{x:Type local:Magnification}" TargetType="{x:Type local:Magnification}">
        <Setter Property="Margin" Value="-1"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <StackPanel  HorizontalAlignment="Left" Background="#404040">
                        <ToggleButton x:Name="ToggleButton1"  Style="{StaticResource ButtonPageControl}" Content="物镜设置" FontSize="14" Foreground="White" IsChecked="True" />
                        <Border CornerRadius="5" Visibility="{Binding ElementName=ToggleButton1 ,Path=IsChecked,Converter={StaticResource BooleanToVisibilityConverter1}}"  Background="#595959" Padding="5" Margin="5,0">
                            <StackPanel x:Name="ObjectiveSettingStackPanel" Orientation="Horizontal">
                                <!--物镜部分-->
                            </StackPanel>
                        </Border>
                        <ToggleButton x:Name="ToggleButton2"  Style="{StaticResource ButtonPageControl}" Content="相机设置" FontSize="14" Foreground="White" IsChecked="True" />
                        <Border CornerRadius="5" Visibility="{Binding ElementName=ToggleButton2 ,Path=IsChecked,Converter={StaticResource BooleanToVisibilityConverter1}}"  Background="#595959" Padding="5" Margin="5,0">
                            <StackPanel Orientation="Horizontal">
                                <Border  Width="300" Height="100" Background="Transparent">

                                </Border>
                            </StackPanel>
                        </Border>

                        <DockPanel>
                            <ToggleButton x:Name="ToggleButton3"  Style="{StaticResource ButtonPageControl}" Content="成像模式" FontSize="14" Foreground="White" IsChecked="True" />
                            <Button Content="重置" Background="#6EA646" BorderBrush="White" BorderThickness="1" Width="50" Foreground="White"  HorizontalAlignment="Center" VerticalAlignment="Center" Margin="80,0,0,0"/>
                            <Button Content="关闭光源" Background="#6EA646" BorderBrush="White" BorderThickness="1" Width="70" Foreground="White"  HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </DockPanel>
                        
                        <Border CornerRadius="5"  Width="300" Visibility="{Binding ElementName=ToggleButton3 ,Path=IsChecked,Converter={StaticResource BooleanToVisibilityConverter1}}" Focusable="True" Background="#595959"  Padding="5" Margin="5,0">
                            <StackPanel>
                                <UniformGrid Columns="6" >
                                    <RadioButton Name="Button31" Tag="0" Style="{StaticResource ToggleButtonStyle1}" Content="明场" Width="45" IsChecked="True" />
                                    <RadioButton Name="Button32" Tag="1" Style="{StaticResource ToggleButtonStyle1}" Content="暗场" Width="45" />
                                    <RadioButton Name="Button33" Tag="2" Style="{StaticResource ToggleButtonStyle1}" Content="莱茵" Width="45" />
                                    <RadioButton Name="Button34" Tag="3" Style="{StaticResource ToggleButtonStyle1}" Content="差分" Width="45" />
                                    <RadioButton Name="Button35" Tag="4" Style="{StaticResource ToggleButtonStyle1}" Content="相位" Width="45" />
                                    <RadioButton Name="Button36" Tag="5" Style="{StaticResource ToggleButtonStyle1}" Content="相差" Width="45" />
                                </UniformGrid>
                                <Border  Visibility="{Binding ElementName=Button31 ,Path=IsChecked,Converter={StaticResource BooleanToVisibilityConverter1}}" Padding="5">
                                    <StackPanel>
                                        <DockPanel>
                                            <TextBlock Text="孔径" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                            <Slider x:Name="Slider311" Style="{StaticResource Horizontal_Slider2}" Value="10" Maximum="20"  TickFrequency="0.02"/>
                                        </DockPanel>
                                        <DockPanel>
                                            <TextBlock Text="亮度" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                            <Slider x:Name="Slider312" Style="{StaticResource Horizontal_Slider2}" Value="45" Maximum="255"  TickFrequency="0.02"/>
                                        </DockPanel>
                                        <DockPanel>
                                            <TextBlock Text="颜色" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                        </DockPanel>
                                    </StackPanel>
                                </Border>
                                <Border  Visibility="{Binding ElementName=Button32 ,Path=IsChecked,Converter={StaticResource BooleanToVisibilityConverter1}}" Padding="5" >
                                    <StackPanel>
                                        <DockPanel>
                                            <TextBlock Text="内径" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                            <Slider x:Name="Slider321" Style="{StaticResource Horizontal_Slider2}" Value="10" Maximum="20"  TickFrequency="0.02"/>
                                        </DockPanel>
                                        <DockPanel>
                                            <TextBlock Text="外径" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                            <Slider x:Name="Slider322" Style="{StaticResource Horizontal_Slider2}" Value="45" Maximum="255"  TickFrequency="0.02"/>
                                        </DockPanel>
                                        <DockPanel>
                                            <TextBlock Text="亮度" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                            <Slider x:Name="Slider323" Style="{StaticResource Horizontal_Slider2}" Value="45" Maximum="255"  TickFrequency="0.02"/>
                                        </DockPanel>
                                        <DockPanel>
                                            <TextBlock Text="伽马" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                            <Slider x:Name="Slider324" Style="{StaticResource Horizontal_Slider2}" Value="0" Maximum="2"  TickFrequency="0.01"/>
                                        </DockPanel>
                                        <DockPanel>
                                            <TextBlock Text="颜色" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                        </DockPanel>
                                        <DockPanel>
                                            <TextBlock Text="自动" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                        </DockPanel>
                                    </StackPanel>
                                </Border>
                                <Border  Visibility="{Binding ElementName=Button33 ,Path=IsChecked,Converter={StaticResource BooleanToVisibilityConverter1}}" Padding="5">
                                    <StackPanel>
                                        <DockPanel>
                                            <TextBlock Text="内径" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                            <Slider x:Name="Slider331" Style="{StaticResource Horizontal_Slider2}" Value="10" Maximum="20"  TickFrequency="0.02"/>
                                        </DockPanel>
                                        <DockPanel>
                                            <TextBlock Text="外径" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                            <Slider x:Name="Slider332" Style="{StaticResource Horizontal_Slider2}" Value="10" Maximum="20"  TickFrequency="0.02"/>
                                        </DockPanel>
                                        <DockPanel>
                                            <TextBlock Text="明场亮度" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                            <Slider x:Name="Slider333" Style="{StaticResource Horizontal_Slider2}" Value="10" Maximum="20"  TickFrequency="0.02"/>
                                        </DockPanel>
                                        <DockPanel>
                                            <TextBlock Text="暗场亮度" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                            <Slider x:Name="Slider334" Style="{StaticResource Horizontal_Slider2}" Value="10" Maximum="20"  TickFrequency="0.02"/>
                                        </DockPanel>
                                        <DockPanel>
                                            <TextBlock Text="伽马" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                            <Slider x:Name="Slider335" Style="{StaticResource Horizontal_Slider2}" Value="0" Maximum="2"  TickFrequency="0.01"/>
                                        </DockPanel>
                                        <DockPanel>
                                            <TextBlock Text="选择图案" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                        </DockPanel>
                                        <DockPanel>
                                            <TextBlock Text="自动旋转" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                        </DockPanel>
                                    </StackPanel>
                                </Border>
                                <Border  Visibility="{Binding ElementName=Button34 ,Path=IsChecked,Converter={StaticResource BooleanToVisibilityConverter1}}" Padding="5">
                                    <StackPanel>
                                        <DockPanel>
                                            <TextBlock Text="相差滤波" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                            <Slider x:Name="Slider341" Style="{StaticResource Horizontal_Slider2}" Value="10" Maximum="20"  TickFrequency="0.02"/>
                                        </DockPanel>
                                        <DockPanel>
                                            <TextBlock Text="对比度" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                            <Slider x:Name="Slider342" Style="{StaticResource Horizontal_Slider2}" Value="10" Maximum="20"  TickFrequency="0.02"/>
                                        </DockPanel>
                                        <DockPanel>
                                            <TextBlock Text="增益" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                            <Slider x:Name="Slider343" Style="{StaticResource Horizontal_Slider2}" Value="10" Maximum="20"  TickFrequency="0.02"/>
                                        </DockPanel>
                                        <DockPanel>
                                            <TextBlock Text="明场权重" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                            <Slider x:Name="Slider344" Style="{StaticResource Horizontal_Slider2}" Value="10" Maximum="20"  TickFrequency="0.02"/>
                                        </DockPanel>
                                        <DockPanel>
                                            <TextBlock Text="背景校正" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                            <ToggleButton Width="50" Template="{StaticResource ToggleButton10}" IsChecked="False" />
                                        </DockPanel>
                                    </StackPanel>
                                </Border>
                                <Border  Visibility="{Binding ElementName=Button35 ,Path=IsChecked,Converter={StaticResource BooleanToVisibilityConverter1}}" Padding="5">
                                    <StackPanel>
                                        <DockPanel>
                                            <TextBlock Text="正则化参数" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                            <Slider x:Name="Slider351" Style="{StaticResource Horizontal_Slider2}" Value="10" Maximum="20"  TickFrequency="0.02"/>
                                        </DockPanel>
                                        <DockPanel>
                                            <TextBlock Text="细节增强" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                            <Slider x:Name="Slider352" Style="{StaticResource Horizontal_Slider2}" Value="10" Maximum="20"  TickFrequency="0.02"/>
                                        </DockPanel>
                                        <DockPanel>
                                            <TextBlock Text="背景校正" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                            <ToggleButton Width="50" Template="{StaticResource ToggleButton10}" IsChecked="False" />
                                        </DockPanel>
                                    </StackPanel>
                                </Border>
                                <Border  Visibility="{Binding ElementName=Button36 ,Path=IsChecked,Converter={StaticResource BooleanToVisibilityConverter1}}" Padding="5">
                                    <StackPanel>
                                        <DockPanel>
                                            <TextBlock Text="对比度" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                            <Slider x:Name="Slider361" Style="{StaticResource Horizontal_Slider2}" Value="10" Maximum="20"  TickFrequency="0.02"/>
                                        </DockPanel>
                                        <DockPanel>
                                            <TextBlock Text="增益" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                            <Slider x:Name="Slider362" Style="{StaticResource Horizontal_Slider2}" Value="10" Maximum="20"  TickFrequency="0.02"/>
                                        </DockPanel>
                                        <DockPanel>
                                            <TextBlock Text="明场权重" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                            <Slider x:Name="Slider363" Style="{StaticResource Horizontal_Slider2}" Value="10" Maximum="20"  TickFrequency="0.02"/>
                                        </DockPanel>
                                        <DockPanel>
                                            <TextBlock Text="背景校正" VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0" Foreground="White"/>
                                            <ToggleButton Width="50" Template="{StaticResource ToggleButton10}" IsChecked="False" />
                                        </DockPanel>
                                    </StackPanel>
                                </Border>
                            </StackPanel>
                        </Border>

                        <ToggleButton x:Name="ToggleButton4"  Style="{StaticResource ButtonPageControl}" Content="载物台控制" FontSize="14" Foreground="White" IsChecked="True" />
                        <Border CornerRadius="5" Width="300"  Visibility="{Binding ElementName=ToggleButton4 ,Path=IsChecked,Converter={StaticResource BooleanToVisibilityConverter1}}"  Background="#595959" Padding="5" Margin="5,0">
                            <StackPanel>
                                <UniformGrid Columns="5" >
                                    <RadioButton Name="Button41" Style="{StaticResource ToggleButtonStyle1}" Content="玻片" Width="50" IsChecked="True" />
                                    <RadioButton Name="Button42" Style="{StaticResource ToggleButtonStyle1}" Content="培养皿" Width="50"  />
                                    <RadioButton Name="Button43" Style="{StaticResource ToggleButtonStyle1}" Content="六口板" Width="50"  />
                                    <RadioButton Name="Button44" Style="{StaticResource ToggleButtonStyle1}" Content="多口版" Width="50"  />
                                    <RadioButton Name="Button45" Style="{StaticResource ToggleButtonStyle1}" Content="自定义" Width="50"  />
                                </UniformGrid>

                                <Border Margin="5" Visibility="{Binding ElementName=Button41 ,Path=IsChecked,Converter={StaticResource BooleanToVisibilityConverter1}}">
                                    <Canvas x:Name="Canvas0"  Visibility="Visible" Height="200" Width="300"   >
                                        <Rectangle Height="100" Width="290" Margin="0,50" Fill="#404040" Stroke="#787878" StrokeThickness="2"/>
                                    </Canvas>
                                </Border>
                                <Border Margin="5" Visibility="{Binding ElementName=Button42 ,Path=IsChecked,Converter={StaticResource BooleanToVisibilityConverter1}}">
                                    <Canvas x:Name="Canvas1"  Visibility="Visible" Height="200" Width="300"   >
                                        <Rectangle Height="100" Width="290" Margin="0,50" Fill="#404040" Stroke="#787878" StrokeThickness="2"/>
                                        <Ellipse x:Name="CricalBase"  Canvas.Top="0" Canvas.Left="50" Width= "200" Height="200"  Fill="#7F7F7F"  Stroke="#787878" StrokeThickness="2"/>
                                        <Line Stroke="white" Canvas.Top="0" Canvas.Left="0" SnapsToDevicePixels="True" StrokeDashArray="5"  StrokeThickness="1" X1="2" Y1="100" X2="288" Y2="100" />
                                        <Line Stroke="White" Canvas.Top="0" Canvas.Left="50" SnapsToDevicePixels="True" StrokeDashArray="4" StrokeThickness="1" X1="100" Y1="0" X2="100" Y2="200"/>
                                    </Canvas>
                                </Border>


                                <ToggleButton x:Name="ToggleButton41"  Style="{StaticResource ButtonPageControl}" Content="XYZ controller" FontSize="13" Foreground="White" IsChecked="True" />
                                <Border  Visibility="{Binding ElementName=ToggleButton41 ,Path=IsChecked,Converter={StaticResource BooleanToVisibilityConverter1}}" Padding="5">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="*"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <ToggleButton x:Name="ToggleButtonXYF" Grid.Row="0" Grid.Column="0" Style="{ StaticResource ToggleButtonStyle1}" Content="F" Margin="5"/>
                                        <TextBlock Grid.Row="3" Grid.Column="0" Text="X-Y控制" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                        <Button x:Name="ButtonFront" Grid.Row="0" Grid.Column="1" Content="︽" Margin="5"/>
                                        <Button x:Name="ButtonRear" Grid.Row="2" Grid.Column="1" Content="︾" Margin="5"/>
                                        <Button x:Name="ButtonLeft" Grid.Row="1" Grid.Column="0" Content="&lt;&lt;" Margin="5"/>
                                        <Button x:Name="ButtonRight" Grid.Row="1" Grid.Column="2"  Content=">>" Margin="5"/>
                                        <Button x:Name="ButtonRe" Grid.Row="1" Grid.Column="1" Content="O" Margin="5"/>

                                        <ToggleButton x:Name="ToggleButtonZF" Grid.Row="0" Grid.Column="3" Style="{ StaticResource ToggleButtonStyle1}" Content="F" Margin="5"/>
                                        <TextBlock Grid.Row="3" Grid.Column="3" Text="Z控制" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Center"/>

                                        <Button x:Name="ButtonUp" Grid.Row="0" Grid.Column="4"  Content="︽" Margin="5"/>
                                        <Button x:Name="ButtonAutoFocus" Grid.Row="1" Grid.Column="4" Content="A" Margin="5"/>
                                        <Button x:Name="ButtonDown" Grid.Row="2" Grid.Column="4"  Content="︾" Margin="5"/>
                                        <Slider x:Name ="Slider1111"  Grid.Row="0" Grid.RowSpan="3" Grid.Column="6"  Style="{StaticResource Horizontal_Slider1}" Minimum="1" Maximum="1000" IsSnapToTickEnabled="True" TickFrequency="1" Value="200" Orientation="Vertical" HorizontalAlignment="Center" />


                                    </Grid>


                                </Border>


                            </StackPanel>

                        </Border>

                        <ToggleButton x:Name="ToggleButton5"  Style="{StaticResource ButtonPageControl}" Content="采集模式" FontSize="14" Foreground="White" IsChecked="True" />
                        <Border CornerRadius="5" Width="300" Visibility="{Binding ElementName=ToggleButton5 ,Path=IsChecked,Converter={StaticResource BooleanToVisibilityConverter1}}"  Background="#595959" Padding="5" Margin="5,0">
                            <StackPanel >
                                <UniformGrid Columns="6"  >
                                    <ToggleButton Style="{StaticResource ToggleButtonStyle1}" Content="X" Width="40" IsChecked="True"/>
                                    <ToggleButton Style="{StaticResource ToggleButtonStyle1}" Content="Y" Width="40" IsChecked="True"/>
                                    <ToggleButton Style="{StaticResource ToggleButtonStyle1}" Content="Z" Width="40" IsChecked="True"/>
                                    <ToggleButton Style="{StaticResource ToggleButtonStyle1}" Content="T" Width="40" IsChecked="True"/>
                                    <ToggleButton Style="{StaticResource ToggleButtonStyle1}" Content="EDOF" Width="40" IsChecked="False"/>
                                    <ToggleButton Style="{StaticResource ToggleButtonStyle1}" Content="自定义" Width="45" />
                                </UniformGrid>
                                <Border Padding="5">
                                    <UniformGrid Columns="2" >
                                        <StackPanel >
                                            <DockPanel >
                                                <TextBlock Text="Z-Begin" Width="50" VerticalAlignment="Center" Foreground="White"/>
                                                <TextBox Text="16" Background="#262626" HorizontalContentAlignment="Center" Foreground="White" Width="40" Margin="5,0"/>
                                                <Button  Content="Set"/>
                                            </DockPanel>
                                            <DockPanel >
                                                <TextBlock Text="Z-End"  Width="50" VerticalAlignment="Center"  Foreground="White"/>
                                                <TextBox Text="32" Background="#262626" HorizontalContentAlignment="Center" Foreground="White" Width="40" Margin="5,0"/>
                                                <Button  Content="Set"/>
                                            </DockPanel>
                                            <DockPanel >
                                                <TextBlock Text="Z-Step"  Width="50" VerticalAlignment="Center" Foreground="White" />
                                                <TextBox Text="16" Background="#262626" HorizontalContentAlignment="Center" Foreground="White" Width="40" Margin="5,0"/>
                                                <Button  Content="Set"/>
                                            </DockPanel>
                                            <DockPanel  HorizontalAlignment="Left">
                                                <TextBlock Text="Z- Absolute"  Width="70" VerticalAlignment="Center" Foreground="White" />
                                            </DockPanel>
                                        </StackPanel>
                                        <StackPanel Margin="5,0,0,0" >
                                            <DockPanel >
                                                <TextBlock Text="T- Number"  Width="65" VerticalAlignment="Center"  Foreground="White"/>
                                                <TextBox Text="16" Background="#262626" HorizontalContentAlignment="Center" Foreground="White" Width="30" Margin="5,0"/>
                                                <Button  Content="Set"/>
                                            </DockPanel>
                                            <DockPanel >
                                                <TextBlock Text="T- Interval"  Width="65" VerticalAlignment="Center" Foreground="White" />
                                                <TextBox Text="16" Background="#262626" HorizontalContentAlignment="Center" Foreground="White" Width="30" Margin="5,0"/>
                                                <Button  Content="Set"/>
                                            </DockPanel>
                                            <DockPanel >
                                                <TextBlock Text="∞"  Width="25" VerticalAlignment="Center" Foreground="White" />
                                                <Slider Value="5" Maximum="10" Style="{StaticResource Horizontal_Slider1}" />
                                                <Button  Content="Set"/>
                                            </DockPanel>
                                            <DockPanel >
                                                <TextBlock Text=">>"  Width="25" VerticalAlignment="Center" Foreground="White" />
                                                <Slider Value="5" Maximum="10" Style="{StaticResource Horizontal_Slider1}" />
                                                <Button  Content="Set"/>
                                            </DockPanel>



                                        </StackPanel>
                                    </UniformGrid>
                                </Border>

                                <ToggleButton x:Name="ToggleButton51"  Style="{StaticResource ButtonPageControl}" Content="Summary" FontSize="13" Foreground="White" IsChecked="True" />
                                <Border Visibility="{Binding ElementName=ToggleButton51 ,Path=IsChecked,Converter={StaticResource BooleanToVisibilityConverter1}}" Padding="5">
                                    <UniformGrid Columns="2">
                                        <StackPanel>
                                            <TextBlock Text="Number of time points: X" FontSize="8" Foreground="#7F7F7F"/>
                                            <TextBlock Text="Number of z-slices: 1" FontSize="8" Foreground="#7F7F7F"/>
                                            <TextBlock Text="Number of xy-positions: 1 " FontSize="8" Foreground="#7F7F7F"/>
                                            <TextBlock Text="Number of Imaging modality: 1" FontSize="8" Foreground="#7F7F7F"/>
                                            <TextBlock Text="Number of Fluor channels: 1" FontSize="8" Foreground="#7F7F7F"/>
                                        </StackPanel>
                                        <StackPanel>
                                            <TextBlock Text="Images Size: 1280×960 (5.2MB)" FontSize="8" Foreground="#7F7F7F"/>
                                            <TextBlock Text="Total Images: 12345" FontSize="8" Foreground="#7F7F7F"/>
                                            <TextBlock Text="Total Memory: 1.546 G" FontSize="8" Foreground="#7F7F7F"/>
                                            <TextBlock Text="Total Duration: 23h 0m 0.002s" FontSize="8" Foreground="#7F7F7F"/>
                                            <TextBlock Text="Total Runtime: 0h 0m 0.002s" FontSize="10" Foreground="#7F7F7F"/>
                                        </StackPanel>



                                    </UniformGrid>
                                </Border>

                            </StackPanel>
                        </Border>

                        <ToggleButton x:Name="ToggleButton6"  Style="{StaticResource ButtonPageControl}" Content="其他设置" FontSize="14" Foreground="White" IsChecked="True" />
                        <Border CornerRadius="5" Visibility="{Binding ElementName=ToggleButton6 ,Path=IsChecked,Converter={StaticResource BooleanToVisibilityConverter1}}"  Background="#595959" Padding="5" Margin="5,0">
                            <StackPanel Orientation="Horizontal">
                                <Border  Width="300" Height="100" Background="Transparent">

                                </Border>
                            </StackPanel>
                        </Border>
                    </StackPanel>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>