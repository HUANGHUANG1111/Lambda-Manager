<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:local="clr-namespace:ConfigCollectInterval">
  <Style x:Key="{x:Type local:CollectInterval}" TargetType="{x:Type local:CollectInterval}">
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type local:CollectInterval}">
          <Viewbox Name="background" Margin="2">
                <GroupBox Header="时间序列" Margin="2" Background="#EEEEF2">
              <StackPanel Orientation="Horizontal">
                <Button Content="无" Width="150" Margin="5,0,5,0" />
                <Button Content="每xx小时xx分钟xx秒" Width="150" Margin="5,0,5,0" />
              </StackPanel>
                        </GroupBox>
          </Viewbox>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
</ResourceDictionary>